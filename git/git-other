//创建一个版本库：
git init 

//chekout 分支的分支
git clone git@git.corp.anjuke.com:aifang/anjuke-oa system
cd system
git checkout system


//rebase master
git checkout master
git pull --rebase
git checkout pmt-10**
git rebase master

//new branch
git checkout -b pmt-

//指定某点重新提交
git cherry-pick 2214e4a

//conflict
edit file
git rebase --continue
git push branch pmt-10***

//////挂起
git stash
git stash apply

//从master上创建分支
git checkout -b pmt-11045 master

//合并分支
git merge pmt-11045 --no-ff
git push origin master

//日志查看
git log
git lg

//查看
git show <>
git branch
git branch -v
git remote -v

//查看冲突文件
git diff

//跳过冲突
git rebase --abort

//回到某个状态
git reflog
git reset --hard 5e5b4d9

//强推
git push branch pmt-11279-update_name -f
做git --rebase origin master后
